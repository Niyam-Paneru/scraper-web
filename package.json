{
  "name": "dental-scraper",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "cd client && npm run dev",
    "postinstall": "cd client && npm install",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "scrape": "node scripts/dental_scraper.js",
    "test": "node --test tests/parsePhone.test.js",
    "install:all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.5",
    "libphonenumber-js": "^1.10.51",
    "mailgun.js": "^12.4.0",
    "moment-timezone": "^0.5.44",
    "playwright": "^1.40.1",
    "robots-parser": "^3.0.1",
    "uuid": "^9.0.1",
    "yargs": "^17.7.2"
  },
  "engines": {
    "node": ">=18"
  }
}
